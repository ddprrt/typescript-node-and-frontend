{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "types": ["node", "express"],
    "lib": ["ESNext"],
    "outDir": "../dist/server",
    "strict": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
